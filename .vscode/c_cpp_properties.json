{
    "configurations": [
        {
            "name": "Mac",
            "includePath": [
                "${workspaceFolder}/app/src/main/cpp/**",
                "${workspaceFolder}/app/src/main/cpp/core",
                "${workspaceFolder}/app/src/main/cpp/conversion",
                "${workspaceFolder}/app/src/main/cpp/hardware",
                "${workspaceFolder}/app/src/main/cpp/inference",
                "${workspaceFolder}/app/src/main/cpp/monitoring",
                "${workspaceFolder}/app/src/main/cpp/benchmark",
                "${workspaceFolder}/app/src/main/cpp/third_party/**",
                "${workspaceFolder}/app/src/main/cpp/third_party/jsoncpp/include",
                "/opt/homebrew/share/android-ndk/toolchains/llvm/prebuilt/darwin-x86_64/include"
            ],
            "defines": ["USE_NEON", "USE_OPENMP"],
            "macFrameworkPath": [
                "/Library/Developer/CommandLineTools/SDKs/MacOSX.sdk/System/Library/Frameworks"
            ],
            "compilerPath": "/usr/bin/clang++",
            "cStandard": "c17",
            "cppStandard": "c++17",
            "intelliSenseMode": "macos-clang-arm64",
            "compileCommands": "${workspaceFolder}/app/src/main/cpp/build/compile_commands.json"
        }
    ],
    "version": 4
}